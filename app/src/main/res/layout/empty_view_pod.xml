<?xml version="1.0" encoding="utf-8"?>
<com.padcmyanmar.padcx.revisionrecyclerview.views.viewPods.EmptyViewPod xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <ImageView
        android:id="@+id/iv_empty"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:src="@drawable/photo3"
        android:scaleType="centerCrop"
        android:layout_centerInParent="true"/>
    <TextView
        android:id="@+id/tv_empty"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="There's no available ,try again."
        android:layout_below="@id/iv_empty"
        android:textAlignment="center"
        android:layout_margin="@dimen/margin_xlarge"
        android:textStyle="bold"/>


</com.padcmyanmar.padcx.revisionrecyclerview.views.viewPods.EmptyViewPod>